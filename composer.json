{
    "name": "loduis/artisan",
    "description": "If you like laravel, but not at the point of implementing it, but you want to keep your organized command, here is a version of artisan with minimum to run",
    "keywords": ["framework", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "league/flysystem": "^1.0.8",
        "monolog/monolog": "^1.12",
        "dragonmantank/cron-expression": "^2.0",
        "nesbot/carbon": "^2.0",
        "symfony/console": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/process": "^4.2",
        "vlucas/phpdotenv": "^3.3",
        "illuminate/config": "5.8.*",
        "illuminate/contracts": "5.8.*",
        "illuminate/container": "5.8.*",
        "illuminate/events": "5.8.*",
        "illuminate/filesystem": "5.8.*",
        "illuminate/log": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/cache": "5.8.*",
        "illuminate/console": "5.8.*",
        "illuminate/translation": "5.8.*",
        "laravel/tinker": "^1.0.8"
    },
    "autoload": {
        "files": [
          "src/Illuminate/Foundation/helpers.php"
        ],
        "psr-4": {
          "Illuminate\\": "src/Illuminate/"
        }
    },
    "bin": [
        "artisan"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "5.8-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
