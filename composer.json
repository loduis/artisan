{
    "name": "loduis/artisan",
    "description": "If you like laravel, but not at the point of implementing it, but you want to keep your organized command, here is a version of artisan with minimum to run",
    "keywords": ["framework", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "league/flysystem": "^1.0.34",
        "monolog/monolog": "^2.0",
        "dragonmantank/cron-expression": "^2.0",
        "nesbot/carbon": "^2.17",
        "symfony/console": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/process": "^5.1",
        "illuminate/config": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/cache": "^8.0",
        "illuminate/console": "^8.0",
        "laravel/tinker": "^2.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
          "Illuminate\\": "src/Illuminate/",
          "Artisan\\": "src/Artisan/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
