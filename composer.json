{
    "name": "loduis/artisan",
    "description": "If you like laravel, but not at the point of implementing it, but you want to keep your organized command, here is a version of artisan with minimum to run",
    "keywords": ["framework", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "league/flysystem": "^1.0.34",
        "monolog/monolog": "^2.0",
        "dragonmantank/cron-expression": "^2.0",
        "nesbot/carbon": "^2.17",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/process": "^5.0",
        "illuminate/config": "7.2.*",
        "illuminate/contracts": "7.2.*",
        "illuminate/container": "7.2.*",
        "illuminate/events": "7.2.*",
        "illuminate/filesystem": "7.2.*",
        "illuminate/log": "7.2.*",
        "illuminate/support": "7.2.*",
        "illuminate/cache": "7.2.*",
        "illuminate/console": "7.2.*",
        "illuminate/translation": "7.2.*",
        "laravel/tinker": "^2.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "autoload": {
        "files": [
          "src/Illuminate/Foundation/helpers.php"
        ],
        "psr-4": {
          "Illuminate\\": "src/Illuminate/"
        }
    },
    "bin": [
        "artisan"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "7.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
