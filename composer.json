{
    "name": "loduis/artisan",
    "description": "If you like laravel, but not at the point of implementing it, but you want to keep your organized command, here is a version of artisan with minimum to run",
    "keywords": ["framework", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "league/flysystem": "~1.0",
        "monolog/monolog": "~1.11",
        "mtdowling/cron-expression": "~1.0",
        "nesbot/carbon": "~1.20",
        "symfony/console": "~3.2",
        "symfony/finder": "~3.2",
        "symfony/http-kernel": "~3.2",
        "symfony/process": "~3.2",
        "vlucas/phpdotenv": "~2.2",
        "paragonie/random_compat": "~1.4|~2.0",
        "illuminate/config": "5.4.*",
        "illuminate/contracts": "5.4.*",
        "illuminate/container": "5.4.*",
        "illuminate/events": "5.4.*",
        "illuminate/filesystem": "5.4.*",
        "illuminate/log": "5.4.*",
        "illuminate/support": "5.4.*",
        "illuminate/cache": "5.4.*",
        "illuminate/console": "5.4.*",
        "laravel/tinker": "~1.0"
    },
    "autoload": {
        "files": [
          "src/Illuminate/Foundation/helpers.php"
        ],
        "psr-4": {
          "Illuminate\\": "src/Illuminate/"
        }
    },
    "bin": [
        "artisan"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "5.4-dev"
        }
    },
    "minimum-stability": "dev"
}
