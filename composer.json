{
    "name": "loduis/artisan",
    "description": "If you like laravel, but not at the point of implementing it, but you want to keep your organized command, here is a version of artisan with minimum to run",
    "keywords": ["framework", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "league/flysystem": "~1.0",
        "psy/psysh": "~0.5.2",
        "monolog/monolog": "~1.11",
        "symfony/console": "3.0.*",
        "symfony/debug": "3.0.*",
        "symfony/finder": "3.0.*",
        "symfony/http-foundation": "3.0.*",
        "symfony/http-kernel": "3.0.*",
        "symfony/process": "3.0.*",
        "vlucas/phpdotenv": "~1.1"
    },
    "replace": {
        "illuminate/config": "self.version",
        "illuminate/console": "self.version",
        "illuminate/container": "self.version",
        "illuminate/events": "self.version",
        "illuminate/exception": "self.version",
        "illuminate/filesystem": "self.version",
        "illuminate/foundation": "self.version",
        "illuminate/log": "self.version",
        "illuminate/support": "self.version"
    },
    "autoload": {
        "files": [
          "src/Illuminate/Foundation/helpers.php",
          "src/Illuminate/Support/helpers.php"
        ],
        "psr-4": {
          "Illuminate\\": "src/Illuminate/"
        }
    },
    "bin": [
        "artisan"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    },
    "minimum-stability": "dev"
}
